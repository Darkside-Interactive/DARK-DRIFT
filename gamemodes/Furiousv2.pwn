public MinServ()//таймер минут на сервере
{
	for(new i=0;i<MAX_PLAYERS;i++)
	{
		if(IsPlayerConnected(i))
		{
			PlayerInfo[i][pMinlog]++;
		}
	}
	if(timecor[8] == 1)//если включено реальное время, то:
	{
		ReadCorTime();//чтение коррекции времени
		gettime(timecor[0], timecor[1]);
		TimCor();//коррекция времени
		if(oldhour != timecor[0])
		{
			oldhour = timecor[0];
			SetWorldTime(timecor[0]);
			GlTime = timecor[0];
			TimePlay(timecor[0]);
		}
	}
	return 1;
}

stock TimePlay(settime)
{
	for(new i=0;i<MAX_PLAYERS;i++)
	{
		if(IsPlayerConnected(i))
		{
			LocTime[i] = settime;
		}
	}
	return 1;
}

public RepairCar()
{
	if(NETdop == 0)//дельта-сепаратор (контроль сети и AFK)
	{
		NETdop = 1;
	}
	else
	{
		NETdop = 0;
	}
    for(new i=0;i<MAX_PLAYERS;i++)
	{
 		if(IsPlayerConnected(i))
 		{
			if(autorepair[i] == 1 && GetPlayerState(i) == 2)//если авторемонт включен,
			{//И игрок на месте водителя, то:
 				new car = GetPlayerVehicleID(i);
 				RepairVehicle(car);
 				SetVehicleHealth(car,1000);
 			}
#if (MOD11INS == 3)
            new swper = 0;//контроль сети и AFK
			NETrecPl[i][NETdop] = NetStats_BytesReceived(i);//общее число считанных байт с сервера
			if(NETdop == 0) {NETrecPl[i][2] = NETrecPl[i][0] - NETrecPl[i][1];}//число считанных "дельта-байт" за единицу времени
			if(NETdop == 1) {NETrecPl[i][2] = NETrecPl[i][1] - NETrecPl[i][0];}
			if(NETrecPl[i][2] < 0) {NETrecPl[i][2] = NETrecPl[i][2] * -1;}//корректировка, если результат в отрицательном диапазоне
			NETsenPl[i][NETdop] = NetStats_BytesSent(i);//общее число переданных байт на сервер
			if(NETdop == 0) {NETsenPl[i][2] = NETsenPl[i][0] - NETsenPl[i][1];}//число переданных "дельта-байт" за единицу времени
			if(NETdop == 1) {NETsenPl[i][2] = NETsenPl[i][1] - NETsenPl[i][0];}
			if(NETsenPl[i][2] < 0) {NETsenPl[i][2] = NETsenPl[i][2] * -1;}//корректировка, если результат в отрицательном диапазоне
			if(NETrecPl[i][2] > NETrecCon)//если считанных байт больше допустимого лимита, то:
			{
				swper = 1;//кик по превышению лимита считанных данных
			}
			if(NETsenPl[i][2] > NETsenCon)//если переданных байт больше допустимого лимита, то:
			{
				swper = 2;//кик по превышению лимита переданных данных
			}
			if(NetStats_MessagesRecvPerSecond(i) <= 1 && PlayerInfo[i][pAdmin] == 0)//если процент считанных сообщений <= 1,
			{//И игрок НЕ админ, то: игрок в AFK
				NETafkPl[i][0]++;//считаем время AFK
				NETafkPl[i][1] = 0;
				if(NETafkPl[i][0] >= 99999999)//если время AFK >= 99999999 секу, то:
				{//кикаем игрока
					NETafkPl[i][0] = 0;//обнуляем счётчик AFK
					PlayerInfo[i][pMinlog] = PlayerInfo[i][pMinlog] - 14;//отнимаем у игрока 14 минут AFK
					swper = 3;//кик по AFK
				}
			}
			else//иначе, игрок НЕ в AFK
			{
				if(NETafkPl[i][1] <= 3 && NETafkPl[i][0] > 0)
				{
					NETafkPl[i][1]++;//задержка выхода из AFK при нестабильных координатах (зависание в воздухе)
					NETafkPl[i][0]++;//считаем время AFK
				}
				else
				{
					NETafkPl[i][0] = 0;//выход из AFK
					NETafkPl[i][1] = 0;
				}
			}
			if(swper != 0)//если указатель НЕ равен 0, то:
			{
				new string[256];
				new sendername[MAX_PLAYER_NAME];
				GetPlayerName(i, sendername, sizeof(sendername));
				switch(swper)
				{
					case 1: format(string, sizeof(string), "Игрок %s [%d] был кикнут - превышен лимит считанных данных с сервера !", sendername, i);
					case 2: format(string, sizeof(string), "Игрок %s [%d] был кикнут - превышен лимит переданных данных на сервер !", sendername, i);
					case 3: format(string, sizeof(string), "Игрок %s [%d] был кикнут - долгое AFK !", sendername, i);
				}
				print(string);
				SendClientMessageToAll(COLOR_RED, string);
				if(playcar[i] != 0)//если у игрока есть транспорт, то:
				{
					new model333;
					model333 = GetVehicleModel(playcar[i]);//чтение модели транспорта игрока
					if(model333 == 432 || model333 == 425 || model333 == 520)//если модель была куплена, то:
					{
						SetPVarInt(i, "PlMon", GetPVarInt(i, "PlMon") + 1000000);//возвращаем стоимость транспорта
						SendClientMessage(i, COLOR_GREEN, "Стоимость транспорта, в размере 1000000 $, была Вам возвращена.");
					}
				}
				SetTimerEx("PlayKick", 300, 0, "i", i);
			}
#endif
		}
	}
	return 1;
}

