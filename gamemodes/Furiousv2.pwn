stock ConvIP(ipadr[])
{
	new ipout[128];
	strdel(ipout, 0, 128);
	new dln, ind;
	dln = strlen(ipadr);
	ind = 0;
	for(new i = 0; i < dln; i++)
	{
		if(ipadr[i] >= 48 && ipadr[i] <= 57)
		{
			ipout[ind] = ipadr[i];
			ind++;
		}
		if(ipadr[i] == '.')
		{
			ipout[ind] = '_';
			ind++;
		}
	}
	return ipout;
}

