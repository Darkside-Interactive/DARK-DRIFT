//==============================================================================
// блокировка дома по его ИД.
//==============================================================================
dcmd_lchouse(playerid, params[])
{
	if(IsPlayerAdmin(playerid) || GetPVarInt(playerid, "AdmLvl") >= 5)
	{
		new file[HOUSEFILE_LENGTH], h;
		if(sscanf(params, "d", h))
		{
			SendClientMessage(playerid, 0xBFC0C2FF, "Используйте: /lchouse [ид дома]");
			SendClientMessage(playerid, 0xFF0000FF, "Внимание !!! Команда блокирует дом по его ID !!!");
			SendClientMessage(playerid, 0xFF0000FF, "Используйте только в КРАЙНЕМ случае !!!");
			return 1;
		}
    	format(file, sizeof(file), FILEPATH, h);
		if(!dini_Exists(file)) return SendClientMessage(playerid, 0xAA3333AA, "Дома с таким ID не существует !");
		if(!strcmp(dini_Get(file, "HouseOwner"), INVALID_HOWNER_NAME, CASE_SENSETIVE)) return SendClientMessage(playerid, 0xAA3333AA, "Нельзя ! Дом свободен !");
		if(!strcmp(dini_Get(file, "HouseOwner"), "* Дом заблокирован", CASE_SENSETIVE)) return SendClientMessage(playerid, 0xAA3333AA, "Дом уже заблокирован !");
		dini_Set(file, "HouseOwner", "* Дом заблокирован");
		dini_Set(file, "HouseName", "* Дом заблокирован");
		UpdateHouseText(h);
		new string[256];
		new sendername[MAX_PLAYER_NAME];
		GetPlayerName(playerid, sendername, sizeof(sendername));
		format(string, sizeof(string), "Администратор %s [%d] заблокировал дом ID %d .", sendername, playerid, h);
		print(string);
		SendAdminMessage(0xAA3333AA, string);
	}
	else
	{
		SendClientMessage(playerid, 0xAA3333AA, " У Вас нет прав на использование этой команды !");
	}
    return 1;
}
//==============================================================================
// This command is used to add a house car for a house.
// The only thing you have to enter is the house value,
// the rest is done by the script.
//==============================================================================
dcmd_creategarage(playerid,params[])
{
	if(!IsPlayerAdmin(playerid)) return 0;
	new locvar1 = -1;//исправления GARAGES
	new locvar2 = 0;//исправления GARAGES
	while(locvar2 < MAX_GARAGES)//исправления GARAGES
	{
		if(garageCount[locvar2] == 0)//исправления GARAGES
		{
			locvar1 = locvar2;//исправления GARAGES
			break;//исправления GARAGES
		}
		locvar2++;//исправления GARAGES
	}
	if(locvar1 == -1) return SendClientMessage(playerid, COLOR_USAGE, "The max. amount of garages is reached. Increase the limit in the jGarage filterscript.");//исправления GARAGES
//	if(garageCount == MAX_GARAGES) return SendClientMessage(playerid, COLOR_USAGE, "The max. amount of garages is reached. Increase the limit in the jGarage filterscript.");//исправления GARAGES
	new price, type;
	if(sscanf(params,"dd",price, type)) return SendClientMessage(playerid, COLOR_USAGE, "Usage: /creategarage <price> <type>  || Use /garagetypes for a list of garage types.");
	if(price < 100) return SendClientMessage(playerid, COLOR_ERROR, "Стоимость гаража не может быть меньше 100$.");
	new Float:X, Float:Y, Float:Z;
	GetPlayerPos(playerid, X,Y,Z);
	format(gInfo[locvar1][Owner],24,"the State");//исправления GARAGES
	gInfo[locvar1][Owned] = 0;//исправления GARAGES
	gInfo[locvar1][Price] = price;//исправления GARAGES
	gInfo[locvar1][gInterior] = type;//исправления GARAGES
	gInfo[locvar1][UID] = locvar1;//исправления GARAGES
	gInfo[locvar1][gPosX] = X;//исправления GARAGES
	gInfo[locvar1][gPosY] = Y;//исправления GARAGES
	gInfo[locvar1][gPosZ] = Z;//исправления GARAGES
	gInfo[locvar1][Locked] = 1;//исправления GARAGES
//	format(gInfo[garageCount][Owner],24,"the State");//исправления GARAGES
//	gInfo[garageCount][Owned] = 0;//исправления GARAGES
//	gInfo[garageCount][Price] = price;//исправления GARAGES
//	gInfo[garageCount][gInterior] = type;//исправления GARAGES
//	gInfo[garageCount][UID] = garageCount;//исправления GARAGES
//	gInfo[garageCount][gPosX] = X;//исправления GARAGES
//	gInfo[garageCount][gPosY] = Y;//исправления GARAGES
//	gInfo[garageCount][gPosZ] = Z;//исправления GARAGES
//	gInfo[garageCount][Locked] = 1;//исправления GARAGES
	new path[128];
	format(path,sizeof(path),"garages/%d.ini",locvar1);//исправления GARAGES //Format the path with the filenumber
//	format(path,sizeof(path),"garages/%d.ini",garageCount);//исправления GARAGES //Format the path with the filenumber
	dini_Create(path);
	Save_Garage(locvar1);//исправления GARAGES
	UpdateGarageInfo(locvar1);//исправления GARAGES
	garageCount[locvar1] = 1;//исправления GARAGES
//	Save_Garage(garageCount);//исправления GARAGES
//	UpdateGarageInfo(garageCount);//исправления GARAGES
//	garageCount++;//исправления GARAGES
	SendClientMessage(playerid,COLOR_SUCCESS,"Garage created!");
	return 1;
}

dcmd_addhcar(playerid, params[])
{
	new file[HOUSEFILE_LENGTH], h;
