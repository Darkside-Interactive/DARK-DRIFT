stock UpdateGarageInfo(gid) //Updates/creates the garage text and label
{
        //Get rid of the old label and pickup (if existing)
        DestroyDynamic3DTextLabel(garageLabel[gid]);
        DestroyDynamicPickup(garagePickup[gid]);
        //DestroyDynamicMapIcon(garageIcon[gid]);//удаляем мап-иконку гаража

        //Re-create them with the correct data
        new ltext[128];
        if(gInfo[gid][Owned] == 1) //If the garage is owned
        {
            format(ltext,128,GARAGE_OWNED_TEXT,gInfo[gid][UID],gInfo[gid][Owner],GetLockedStatus(gInfo[gid][Locked]));
            garageLabel[gid] = CreateDynamic3DTextLabel(ltext, TXTCOLOR, gInfo[gid][gPosX],gInfo[gid][gPosY],gInfo[gid][gPosZ]+0.1,DD);
            garagePickup[gid] = CreateDynamicPickup(GARAGE_OWNED_PICKUP,1,gInfo[gid][gPosX],gInfo[gid][gPosY],gInfo[gid][gPosZ]+0.2);
        }
        if(gInfo[gid][Owned] == 0)
        {
            format(ltext,128,GARAGE_FREE_TEXT,gInfo[gid][UID],gInfo[gid][Price]);
            garageLabel[gid] = CreateDynamic3DTextLabel(ltext, TXTCOLOR, gInfo[gid][gPosX],gInfo[gid][gPosY],gInfo[gid][gPosZ]+0.1,DD);
            garagePickup[gid] = CreateDynamicPickup(GARAGE_FREE_PICKUP,1,gInfo[gid][gPosX],gInfo[gid][gPosY],gInfo[gid][gPosZ]);
        }
}

