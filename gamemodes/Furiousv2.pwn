public OnGameModeExit()
{
    Save_Garages();
    SaveProperties();
//==============================================================================
//                              GarHouse
//==============================================================================
	new file[HOUSEFILE_LENGTH], tmp;
	Loop(i, MAX_PLAYERS)
	{
	    if(IsPlayerConnected(i) && !IsPlayerNPC(i))
	    {
	        tmp = GetPVarInt(i, "LastHouseCP");
			format(file, sizeof(file), FILEPATH, tmp);
		    if(!strcmp(GetHouseOwner(tmp), pNick(i), CASE_SENSETIVE) && GetPVarInt(i, "IsInHouse") == 1 && dini_Exists(file))
			{
			    dini_IntSet(file, "QuitInHouse", 1);
			    #if defined GH_HOUSECARS
			    	SaveHouseCar(tmp);
		        #endif
			}
		}
	}
    UnloadHouses(); // Выгрузка домов (также разгружает дом автомобили)
//==============================================================================
//                              End GarHouse
//==============================================================================

	for(new i = 0; i < MAX_PLAYERS; i++)//speedometer
	{
	    TextDrawHideForPlayer(i, VehicleSpeed[i]);
	    TextDrawHideForPlayer(i, KMH[i]);
	}

//------------------------------- Elevator -------------------------------------
	DestroyObject(Obj_Elevator);
	DestroyObject(Obj_ElevatorDoors[0]);
	DestroyObject(Obj_ElevatorDoors[1]);
	Delete3DTextLabel(Label_Elevator);

	for(new i; i < sizeof(Obj_FloorDoors); i ++)
	{
	    DestroyStreamObject(Obj_FloorDoors[i][0]);
		DestroyStreamObject(Obj_FloorDoors[i][1]);
		Delete3DTextLabel(Label_Floors[i]);
		Delete3DTextLabel(Label_Floors22[i]);
	}
//----------------------------- End Elevator -----------------------------------

	for(new i = 0; i < MAX_PLAYERS; i++)//удаление мап иконок наблюдения
	{
    	if(mapiconid[i] != -600)//если ID мап иконки наблюдения НЕ пустой, то:
    	{
			DestroyDynamicMapIcon(mapiconid[i]);//удаление мап иконки наблюдения
		}
	}
	return 1;
}
